fun on-reset ( -> ) {
	@block {
		jump @block&ptr

		"string"
		"привет!!"
		'c''\n'

		0xff* 2* sub

		0 if { load-k }
		else { store-r }
		pop-rk
		pop-kr

		while 1 {}

		loop @b {}

		return

		@a {
			0 if {jump @a}
			jump@block
		}
	}

	0*return
}

