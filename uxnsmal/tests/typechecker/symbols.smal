// Functions
fun vec ( -> ) {}
fun proc ( -- byte ) { 10 }

fun proc2 ( -- ) {}
fun proc3 ( byte -- short ) { pop 20* }
fun proc4 ( byte -- fun(byte -- short) ) { pop &proc3 }

fun t1( -- fun(->)      ) { &vec }
fun t2( -- fun(-- byte) ) { &proc }

// Variables
var byte         my-byte
var short        my-short
var fun(->)      my-vec-ptr
var fun(-- byte) my-proc-ptr
var *byte        my-ptr

fun t3( -- byte         ) { my-byte }
fun t4( -- short        ) { my-short }
fun t5( -- fun(->)      ) { my-vec-ptr }
fun t6( -- fun(-- byte) ) { my-proc-ptr }
fun t7( -- *byte        ) { my-ptr }

fun t8 ( -- ^byte         ) { &my-byte }
fun t9 ( -- ^short        ) { &my-short }
fun t10( -- ^fun(->)      ) { &my-vec-ptr }
fun t11( -- ^fun(-- byte) ) { &my-proc-ptr }
fun t12( -- ^*byte        ) { &my-ptr }

fun t13( -- byte         ) { &my-byte load }
fun t14( -- short        ) { &my-short load }
fun t15( -- fun(->)      ) { &my-vec-ptr load }
fun t16( -- fun(-- byte) ) { &my-proc-ptr load }
fun t17( -- *byte        ) { &my-ptr load }

fun t18( byte         -- ) { &my-byte store }
fun t19( short        -- ) { &my-short store }
fun t20( fun(->)      -- ) { &my-vec-ptr store }
fun t21( fun(-- byte) -- ) { &my-proc-ptr store }
fun t22( *byte        -- ) { &my-ptr store }

// Constants
const byte         MY_BYTE     { 0xff }
const short        MY_SHORT    { 0xffff* }
const fun(->)      MY_VEC_PTR  { &vec }
const fun(-- byte) MY_PROC_PTR { &proc }
const *byte        MY_PTR      { &my-data }

fun t23( -- byte         ) { MY_BYTE }
fun t24( -- short        ) { MY_SHORT }
fun t25( -- fun(->)      ) { MY_VEC_PTR }
fun t26( -- fun(-- byte) ) { MY_PROC_PTR }
fun t27( -- *byte        ) { MY_PTR }

// Datas
data my-data { $8 }

fun t28( -- byte  ) { my-data }
fun t29( -- *byte ) { &my-data }
fun t30( -- byte  ) { &my-data load }
fun t31( byte --  ) { &my-data store }
fun t32( byte --  ) { &my-data 2* add store }
