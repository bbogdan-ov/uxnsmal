// Pop
fun _( byte         byte    -- byte         ) { pop }
fun _( short        short   -- short        ) { pop }
fun _( fun(->)      fun(->) -- fun(->)      ) { pop }
fun _( byte         ^short  -- byte         ) { pop }
fun _( short        *byte   -- short        ) { pop }
fun _( *short       *byte   -- *short       ) { pop }
fun _( fun(->)      *byte   -- fun(->)      ) { pop }
fun _( fun(-- byte) fun(->) -- fun(-- byte) ) { pop }

// Swap
fun _( byte         byte    -- byte byte            ) { swap }
fun _( short        short   -- short short          ) { swap }
fun _( fun(->)      fun(->) -- fun(->) fun(->)      ) { swap }
fun _( byte         ^short  -- ^short byte          ) { swap }
fun _( short        *byte   -- *byte short          ) { swap }
fun _( *short       *byte   -- *byte *short         ) { swap }
fun _( fun(->)      *byte   -- *byte fun(->)        ) { swap }
fun _( fun(-- byte) fun(->) -- fun(->) fun(-- byte) ) { swap }

// Nip
fun _( byte         byte    -- byte    ) { nip }
fun _( short        short   -- short   ) { nip }
fun _( fun(->)      fun(->) -- fun(->) ) { nip }
fun _( byte         ^short  -- ^short  ) { nip }
fun _( short        *byte   -- *byte   ) { nip }
fun _( *short       *byte   -- *byte   ) { nip }
fun _( fun(->)      *byte   -- *byte   ) { nip }
fun _( fun(-- byte) fun(->) -- fun(->) ) { nip }

// Rot
fun _( byte    byte         byte    -- byte         byte    byte    ) { rot }
fun _( short   short        short   -- short        short   short   ) { rot }
fun _( fun(->) fun(->)      fun(->) -- fun(->)      fun(->) fun(->) ) { rot }
fun _( byte    ^short       ^byte   -- ^short       ^byte   byte    ) { rot }
fun _( short   *byte        fun(->) -- *byte        fun(->) short   ) { rot }
fun _( *short  *byte        short   -- *byte        short   *short  ) { rot }
fun _( fun(->) *byte        fun(--) -- *byte        fun(--) fun(->) ) { rot }
fun _( fun(--) fun(-- byte) fun(->) -- fun(-- byte) fun(->) fun(--) ) { rot }

// Dup
fun _( byte         -- byte byte                 ) { dup }
fun _( short        -- short short               ) { dup }
fun _( fun(->)      -- fun(->) fun(->)           ) { dup }
fun _( byte         -- byte byte                 ) { dup }
fun _( short        -- short short               ) { dup }
fun _( *short       -- *short *short             ) { dup }
fun _( fun(->)      -- fun(->) fun(->)           ) { dup }
fun _( fun(-- byte) -- fun(-- byte) fun(-- byte) ) { dup }

// Over
fun _( byte         byte    -- byte         byte    byte         ) { over }
fun _( short        short   -- short        short   short        ) { over }
fun _( fun(->)      fun(->) -- fun(->)      fun(->) fun(->)      ) { over }
fun _( byte         ^short  -- byte         ^short  byte         ) { over }
fun _( short        *byte   -- short        *byte   short        ) { over }
fun _( *short       *byte   -- *short       *byte   *short       ) { over }
fun _( fun(->)      *byte   -- fun(->)      *byte   fun(->)      ) { over }
fun _( fun(-- byte) fun(->) -- fun(-- byte) fun(->) fun(-- byte) ) { over }

