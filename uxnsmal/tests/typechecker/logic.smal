type byte MyType
fun usertype1 ( MyType MyType -- byte ) { eq }
fun usertype2 ( byte MyType   -- byte ) { as (MyType MyType) eq }
fun usertype3 ( byte byte     -- byte ) { as (MyType MyType) eq }

