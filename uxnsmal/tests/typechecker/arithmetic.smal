// Add
fun t1( byte byte     -- byte  ) { add }
fun t2( short short   -- short ) { add }

fun t3( byte ^byte    -- ^byte   ) { add }
fun t4( short *byte   -- *byte   ) { add }
fun t5( short fun(->) -- fun(->) ) { add }
fun t6( ^byte byte    -- ^byte   ) { add }
fun t7( *byte short   -- *byte   ) { add }
fun t8( fun(->) short -- fun(->) ) { add }

fun t9 ( ^short ^short   -- ^short  ) { add }
fun t10( *short *short   -- *short  ) { add }
fun t11( fun(->) fun(->) -- fun(->) ) { add }

fun t12( fun(byte --) fun(byte --)     -- fun(byte --)        ) { add }
fun t13( fun(^short --) fun(^short --) -- fun(^short --)      ) { add }
fun t14( short fun(^short -- byte)     -- fun(^short -- byte) ) { add }

// Inc
fun t15( byte         -- byte         ) { inc }
