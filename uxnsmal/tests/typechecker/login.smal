// And
fun _( byte byte   -- byte  ) { and }
fun _( short short -- short ) { and }

// Or
fun _( byte byte   -- byte  ) { or }
fun _( short short -- short ) { or }

// Xor
fun _( byte byte   -- byte  ) { xor }
fun _( short short -- short ) { xor }

// Eq
fun _( byte byte             -- byte ) { eq }
fun _( short short           -- byte ) { eq }
fun _( ^byte ^byte           -- byte ) { eq }
fun _( *short *short         -- byte ) { eq }
fun _( fun(->) fun(->)       -- byte ) { eq }
fun _( ^byte ^short          -- byte ) { eq }
fun _( *byte *short          -- byte ) { eq }
fun _( fun(->) fun(short --) -- byte ) { eq }
fun _( *fun(->) *short       -- byte ) { eq }

// All these tests below are the same as tests for the `eq` intrinsic

// Neq
